{
  "emmet.preferences": {},
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.fontFamily": "Cascadia Code",
  "editor.fontLigatures": true,
  "workbench.iconTheme": "material-icon-theme",
  "workbench.statusBar.visible": false,
  "emmet.excludeLanguages": ["markdown"],
  "emmet.triggerExpansionOnTab": true,
  "material-ui-snippets.importPaths": "top level",
  "material-ui-snippets.showNotesOnStartup": false,
  "terminal.integrated.fontFamily": "Meslo LG M for Powerline",
  "terminal.explorerKind": "external",
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "bracket-pair-colorizer-2.depreciation-notice": false,
  "workbench.startupEditor": "none",
  "editor.foldingMaximumRegions": 20000,
  "editor.multiCursorMaxLines": 20000,
  "glassit.alpha": 80,
  "vscode_vibrancy.opacity": 0.03,
  "terminal.integrated.gpuAcceleration": "off",
  "vscode_vibrancy.type": "ultra-dark",
  "workbench.colorTheme": "Community Material Theme",
  "[python]": {
    "editor.defaultFormatter": "ms-python.python",
    "editor.formatOnType": true
  },
  "[prisma]": {
    "editor.defaultFormatter": "Prisma.prisma"
  },
  "tailwindCSS.includeLanguages": {
    "javascript": "javascript",
    "html": "HTML",
    "typescript": "typescript"
  },

  "editor.quickSuggestions": {
    "strings": true
  },
  "tailwindCSS.experimental.classRegex": [],
  "files.autoSave": "afterDelay",
  "nim.project": [],
  "editor.inlineSuggest.enabled": true,
  "kotlin.languageServer.enabled": false,
  "kotlin.debugAdapter.enabled": false,
  "[kotlin]": {
    "editor.defaultFormatter": "esafirm.kotlin-formatter"
  },
  "workbench.preferredDarkColorTheme": "Community Material Theme",
  "workbench.preferredHighContrastColorTheme": "Community Material Theme",
  "workbench.preferredHighContrastLightColorTheme": "Community Material Theme",
  "workbench.preferredLightColorTheme": "Community Material Theme",
  "material-icon-theme.activeIconPack": "react",
  "vscode_vibrancy.refreshInterval": 100,
  "editor.minimap.enabled": false,
  "vscode_vibrancy.theme": "Default Dark",
  "terminal.integrated.env.osx": {},
  "editor.fontSize": 15,
  "editor.codeActionsOnSave": {}
}
